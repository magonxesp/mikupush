{
  "name": "example_electron",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix",
    "vite:dev": "vite",
    "vite:build": "vite build",
    "electron:dev": "electronmon . --dev-server",
    "electron:prod": "electron .",
    "dev": "concurrently \"npm run vite:dev\" \"wait-on http://localhost:5173 && npm run electron:dev\""
  },
  "dependencies": {
    "@material/web": "^2.2.0",
    "@mdi/js": "^7.4.47",
    "vue": "^3.5.13",
    "vuetify": "^3.7.15"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "concurrently": "^9.1.2",
    "electron": "^35.0.0",
    "electronmon": "^2.0.3",
    "material-design-icons-iconfont": "^6.7.0",
    "standard": "^17.1.2",
    "vite": "^6.2.0",
    "wait-on": "^8.0.2"
  }
}

{
	"name": "mikupush",
	"productName": "Miku Push",
	"author": "MagonxESP",
	"description": "Simple, fast, and open-source alternative to WeTransfer, available for Windows, macOS, and Linux. It doesn't require registration and allows anonymous file sharing by default, ensuring privacy and ease of use from the very first moment.",
	"private": true,
	"version": "1.1.0",
	"type": "commonjs",
	"main": "dist-electron/main.js",
	"license": "Apache-2.0",
	"scripts": {
		"dev": "vite",
		"build:dev": "tsc -b && vite build --mode development",
		"build:prod": "tsc -b && vite build",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"preview": "vite preview",
		"package": "electron-forge package",
		"start": "electron-forge start",
		"make": "electron-forge make",
		"make:dev": "cross-env BUILD_ENV=DEV electron-forge make"
	},
	"dependencies": {
		"@fortawesome/free-brands-svg-icons": "^6.7.2",
		"@fortawesome/free-regular-svg-icons": "^6.7.2",

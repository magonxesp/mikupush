{
  "name": "example_electron",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "lint": "eslint ./src ./main.ts",
    "lint:fix": "eslint --fix ./src ./main.ts",
    "vite:dev": "vite",
    "vite:build": "vite build",
    "electron:dev": "tsc --outdir dist -p tsconfig.electron.json && electron dist/main.js --dev-server",
    "electron:prod": "tsc --outdir dist -p tsconfig.electron.json && electron dist/main.js",
    "dev": "concurrently \"npm run vite:dev\" \"wait-on http://localhost:5173 && npm run electron:dev\""
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@material/web": "^2.2.0",
    "@mdi/js": "^7.4.47",
    "@vue/eslint-config-typescript": "^14.5.0",
    "axios": "^1.8.2",
    "file-type": "^20.4.0",
    "pinia": "^3.0.1",
    "uuid": "^11.1.0",
    "vue": "^3.5.13",
    "vuetify": "^3.7.15"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "concurrently": "^9.1.2",
    "electron": "^35.0.0",
    "electronmon": "^2.0.3",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "^10.0.0",
    "material-design-icons-iconfont": "^6.7.0",
    "neostandard": "^0.12.1",
    "typescript": "^5.8.2",
    "vite": "^6.2.0",
    "wait-on": "^8.0.2"
  }
}

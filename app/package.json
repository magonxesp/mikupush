{
  "name": "mikupush",
  "productName": "Miku Push",
  "appVersion": "1.0.0",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build:dev": "tsc -b && vite build --mode development",
    "build:prod": "tsc -b vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "package:macos": "electron-packager . --platform=darwin --arch=x64,arm64 --out=target --overwrite --icon=resources/macOS/AppIcon.icns --app-bundle-id=io.mikupush.MikuPush --asar",
    "package:windows": "electron-packager . --platform=win32 --arch=x64,arm64 --out=target --overwrite --icon=resources/windows/icon.ico --asar",
    "package:linux": "electron-packager . --platform=linux --arch=x64,arm64 --out=target --overwrite --icon=resources/linux/icon.png --asar",
    "package": "npm run package:macos && npm run package:windows && npm run package:linux"
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@material/web": "^2.2.0",
    "axios": "^1.8.2",
    "file-type": "^20.4.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sequelize": "^6.37.6",
    "sqlite3": "^5.1.7",
    "use-debounce": "^10.0.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@electron/packager": "^18.3.6",
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "cross-env": "^7.0.3",
    "electron": "^35.0.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-native": "^2.2.2",
    "vite-plugin-static-copy": "^2.3.1"
  }
}
